<h1 class="display-4">Order List</h1>
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>Quantity</th>
            <th>Total Price</th>
          </tr>
        </thead>

        <tbody>
          <% @orders.each do |order| %>
          <td><%= order.qty %></td>
          <td><%= order.total_price %></td>
          <% end %>
        </tbody>
      </table>